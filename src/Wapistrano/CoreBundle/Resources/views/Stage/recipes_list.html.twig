<table id="dataTable" class="table table-bordered table-condensed table-hover table-striped">
    <thead>
    <tr>
        <th>Name</th>
        <th>Description</th>
    </tr>
    </thead>
    <tbody>
{% for recipe in recipes %}
    <tr>
        <td>{{ recipe.name }}</td>
        <td>{{ recipe.description }}</td>
    </tr>
{% endfor %}
    </tbody>
</table>

